{
    "version": 2,
    "builds": [
      {
        "src": "api/app.py",
        "use": "@vercel/python",
        "config": { "runtime": "python3.9" }
      }
    ],
    "routes": [
      {
        "src": "/",
        "methods": ["POST", "GET"],
        "dest": "/api/app.py",
        "headers":  {
          "access-control-allow-origin": "*",
          "access-control-allow-methods": "GET, POST",
          "access-control-allow-headers": "Content-Type"
        }
      },
      {
        "src": "/sobre",
        "methods": ["POST", "GET"],
        "dest": "/api/app.py",
        "headers":  {
          "access-control-allow-origin": "*",
          "access-control-allow-methods": "GET, POST",
          "access-control-allow-headers": "Content-Type"
        }
      },
      {
        "src": "/imagetopython",
        "methods": ["POST"],
        "dest": "/api/app.py",
        "headers":  {
          "access-control-allow-origin": "*",
          "access-control-allow-methods": "POST",
          "access-control-allow-headers": "Content-Type"
        }
      }
    ]
  }